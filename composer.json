{
  "name": "tourze/workerman-user-support",
  "description": "为Workerman连接增加用户支持",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "psr/log": "^3|^2|^1",
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "tourze/workerman-psr-logger": "0.0.*",
    "workerman/workerman": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\UserSupport\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\UserSupport\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}